<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Dark Code">
  <style name="Text" foreground="#f0f0f0" background="#141414"/>
  <style name="Link" foreground="#0000ff"/>
  <style name="Selection" background="#00557f"/>
  <style name="LineNumber" foreground="#808080" background="#282828"/>
  <style name="SearchResult" foreground="#000000" background="#ffef0b"/>
  <style name="SearchScope" background="#aaaa00"/>
  <style name="Parentheses" foreground="#ff0000" background="#b4eeb4"/>
  <style name="CurrentLine" background="#282828"/>
  <style name="CurrentLineNumber" foreground="#c8c8c8" bold="true"/>
  <style name="Occurrences" background="#aa557f"/>
  <style name="Occurrences.Unused" foreground="#808000"/>
  <style name="Occurrences.Rename" background="#ff557f"/>
  <style name="Number" foreground="#00aa7f"/>
  <style name="String" foreground="#00aa00"/>
  <style name="Type" foreground="#aa00ff"/>
  <style name="Local" foreground="#ff5500"/>
  <style name="Field" foreground="#ff5500" italic="true"/>
  <style name="Static" foreground="#aa55ff"/>
  <style name="VirtualMethod" foreground="#00aaff" italic="true"/>
  <style name="Function" foreground="#00aaff"/>
  <style name="Keyword" foreground="#ffff7f"/>
  <style name="PrimitiveType" foreground="#808000"/>
  <style name="Operator" foreground="#ececec"/>
  <style name="Preprocessor" foreground="#00aaff"/>
  <style name="Label" foreground="#ff5500"/>
  <style name="Comment" foreground="#808080"/>
  <style name="Doxygen.Comment" foreground="#aaaa7f"/>
  <style name="Doxygen.Tag" foreground="#aaaa00"/>
  <style name="VisualWhitespace" foreground="#464646"/>
  <style name="QmlLocalId" foreground="#ffaa00" italic="true"/>
  <style name="QmlExternalId" foreground="#ffaa00" italic="true"/>
  <style name="QmlTypeId" foreground="#aa00ff"/>
  <style name="QmlRootObjectProperty" foreground="#ff5500" italic="true"/>
  <style name="QmlScopeObjectProperty" foreground="#ff5500" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#ff5500" italic="true"/>
  <style name="JsScopeVar" foreground="#ff5500" italic="true"/>
  <style name="JsImportVar" foreground="#ffaaff" italic="true"/>
  <style name="JsGlobalVar" foreground="#ff5500" italic="true"/>
  <style name="QmlStateName" foreground="#00aa00" italic="true"/>
  <style name="Binding" foreground="#ff5500"/>
  <style name="DisabledCode" foreground="#000000" background="#efefef"/>
  <style name="AddedLine" foreground="#00aa00"/>
  <style name="RemovedLine" foreground="#ff0000"/>
  <style name="DiffFile" background="#aa5500"/>
  <style name="DiffLocation" background="#55007f"/>
  <style name="DiffFileLine" background="#aaaa00"/>
  <style name="DiffContextLine" background="#00007f"/>
  <style name="DiffSourceLine" background="#ff0000"/>
  <style name="DiffSourceChar" background="#aa0000"/>
  <style name="DiffDestLine" background="#00aa00"/>
  <style name="DiffDestChar" background="#005500"/>
